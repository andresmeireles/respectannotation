{
    "name": "andresmeireles/respectannotation",
    "type": "library",
    "description": "use validations of Respect Validation as annotations in doctrine entities",
    "keywords": [
        "andresmeireles",
        "respectannotations",
        "respect",
        "doctrine",
        "annotations"
    ],
    "homepage": "https://github.com/andresmeireles/respectannotation",
    "license": "MIT",
    "authors": [
        {
            "name": "Andr√© Meireles",
            "email": "andresmeireles@gmail.com",
            "homepage": "https://andresmeireles.github.io/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Andresmeireles\\RespectAnnotation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Andresmeireles\\RespectAnnotation\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
